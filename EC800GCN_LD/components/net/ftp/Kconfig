menu "ftp"

config FTP_MODULE_SUPPORT
    bool "FTP_MODULE_SUPPORT enabled"
    default y
    depends on NET_LWIP_TCP_ON
    help
        When enabled, FTP_MODULE_SUPPORT will be defined

config FTP_OVER_SSL_SUPPORT
    bool "FTP_OVER_SSL_SUPPORT enabled"
    default y
    depends on FTP_MODULE_SUPPORT && SOC_8910
    help
        When enabled, FTP_OVER_SSL_SUPPORT will be defined


endmenu
